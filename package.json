{
  "name": "unocss-nextjs",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{components,app,pages,styles}/**/*.*": "pnpm run precommit"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fix:lint": "next lint --fix",
    "fmt": "prettier --write **/*.*",
    "precommit": "pnpm fix:lint && pnpm fmt",
    "generate": "npx scaffdog generate"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^5.40.0",
    "@unocss/transformer-attributify-jsx": "^0.45.29",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sort-keys-custom-order": "^1.0.3",
    "eslint-plugin-strict-dependencies": "^1.0.1",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "eslint-plugin-unocss": "0.0.1-alpha.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "next": "13.0.2",
    "prettier": "^2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.8",
    "@types/node": "18.8.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@unocss/preset-attributify": "0.45.29",
    "@unocss/preset-tagify": "0.45.29",
    "@unocss/reset": "0.45.29",
    "@unocss/webpack": "0.45.29",
    "eslint": "8.25.0",
    "eslint-config-next": "13.0.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "scaffdog": "^2.4.0",
    "typescript": "4.8.4",
    "unocss": "0.45.29"
  }
}
